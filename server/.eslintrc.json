{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent":[
            "error",
            2
        ],
        "linebreak-style":"off",
        "quotes":[
            "error",
            "single"
        ],
        "require-jsdoc":"off",
        "new-cap":"off"
    }
}
